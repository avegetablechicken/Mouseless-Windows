      "right_windows": "right_control"
    }
  },

  "hyper": {
    "hyper": "F18",
    "app": ["alt", "control"],
    "daemon": ["shift", "alt"],
    "controlCenter": ["shift", "win"],
    "moveTo": ["alt", "control"],
    "moveTowards": ["shift", "alt", "control"],
    "moveToDesktop": ["win", "control"],
    "focusDesktop": "win"
